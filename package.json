{
  "dependencies": {
    "cors": "^2.8.5",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "events": "^3.3.0",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "express-sse": "0.5.1",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "nodemon": "^3.0.1",
    "path": "^0.12.7",
    "pg": "^8.11.3",
    "sqlstring": "^2.3.3"
  },
  "devDependencies": {
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^9.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.1"
  },
  "scripts": {
    "start": "nodemon server.js",
    "format:check": "prettier --check .  --config ./.prettierrc.json",
    "format:write": "prettier --write . --config ./.prettierrc.json",
    "lint:check": "eslint .",
    "lint:fix": "eslint --fix .",
    "lint": "eslint --quiet --fix",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "npm run lint",
      "prettier --write --config ./.prettierrc.json"
    ]
  }
}
